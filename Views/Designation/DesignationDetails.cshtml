@model MyMCE.Models.Domain.Designations.tbl_MCE_Designations

@{
    ViewBag.Title = "Designation Details";
    Layout = "~/Views/Shared/_LayoutAdminLte.cshtml";
}

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="row">
        <div class="col-sm-3">
            <div class="form-group">
                @Html.HiddenFor(model => model.intDesignationID)

                @Html.LabelFor(model => model.intDesignationID)
                @Html.EditorFor(model => model.intDesignationID, new { htmlAttributes = new { @class = "form-control form-control-sm", @disabled = "disabled" } })
                @Html.ValidationMessageFor(model => model.intDesignationID, "", new { @class = "text-danger" })

            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                @Html.LabelFor(model => model.vchDesignation)

                @Html.EditorFor(model => model.vchDesignation, new { htmlAttributes = new { @class = "form-control form-control-sm", @disabled = "disabled" } })
                @Html.ValidationMessageFor(model => model.vchDesignation, "", new { @class = "text-danger" })

            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                @Html.LabelFor(model => model.vchDesignationDescription)

                @Html.EditorFor(model => model.vchDesignationDescription, new { htmlAttributes = new { @class = "form-control form-control-sm", @disabled = "disabled" } })
                @Html.ValidationMessageFor(model => model.vchDesignationDescription, "", new { @class = "text-danger" })

            </div>
        </div>
        <div class="col-sm-3">
            <div class="form-group">
                @Html.LabelFor(model => model.vchRemarks)

                @Html.EditorFor(model => model.vchRemarks, new { htmlAttributes = new { @class = "form-control form-control-sm", @disabled = "disabled" } })
                @Html.ValidationMessageFor(model => model.vchRemarks, "", new { @class = "text-danger" })

            </div>
        </div>
    </div>
    <div class="card-footer">
        <h3 class="btn btn-default float-right">@Html.ActionLink("Back to List", "ViewDesignationDataTableWithTab")</h3>

    </div>
}

